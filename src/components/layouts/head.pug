mixin head()
  meta(charset = "utf-8")
  meta(
    name = "viewport",
    content = "width=device-width, initial-scale=1.0"
  )

  meta(
    http-equiv = "X-UA-Compatible",
    content = "ie=edge"
  )

  title #{title}

  meta(
    name = "description",
    content = description
  )

  script(
    src = 'https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js'
  )
